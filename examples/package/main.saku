from core import io
from core import integer
from core import string

fact n =
    aux n acc =
        match n with
        | 0 -> acc
        | k -> match integer.gt k 0 with
            | true -> aux (integer.sub k 1) (integer.mul n acc)
            | false -> io.panic "negative input"
    in
    aux n 1

_ =
    n = io.input "factorial n: " in
    io.println (string.from_int (fact n))
